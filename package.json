{
  "name": "@turnly/athena",
  "version": "1.0.0",
  "private": true,
  "description": "Queue management system for waiting lines & walk-ins at physical locations.",
  "workspaces": {
    "packages": [
      "apps/*"
    ],
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "docker": "bash ./scripts/run.sh docker",
    "prepare": "husky install",
    "start": "bash ./scripts/run.sh develop"
  },
  "commitlint": {
    "extends": [
      "@turnly/eslint-config/commits/commitlint.maverick.js"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@turnly/eslint-config": "git+https://turnlyapps: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@github.com/turnly/configs.git",
    "husky": "^8.0.1"
  },
  "engines": {
    "node": ">=16.15.0"
  }
}
