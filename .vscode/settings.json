    "**/tsconfig.tsbuildinfo": true,
    "**/yarn-error.log": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.workingDirectories": [
    "apps/Maverick",
    "apps/heimdall",
    "apps/Wayne",
    "apps/Alfred",
    "apps/Shannon",
    "apps/Dorothee",
    "apps/queuing-system",
    "apps/Curry",
    "apps/Chanel",
    "apps/Brennan",
    "apps/Polly",
