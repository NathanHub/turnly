{
  "debug.javascript.unmapMissingSources": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.exclude": {
    "**/.editorconfig": true,
    "**/.eslint*": true,
    "**/.git": true,
    "**/.husky": true,
    "**/.nvmrc": true,
    "**/.prettier*": true,
    "**/dist": true,
    "**/jest*": true,
